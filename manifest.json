{
  "manifest_version": 3,
  "name": "Web Radio Player",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "r_vec.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["offscreen"],
  "web_accessible_resources": [{
    "resources": ["stations.json", "offscreen.html"],
    "matches": ["<all_urls>"]
  }]
}